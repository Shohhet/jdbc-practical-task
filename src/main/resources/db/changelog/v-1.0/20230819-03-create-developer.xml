<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <changeSet  id="3"  author="shohhet">
        <createTable  tableName="developerEntity">
            <column  name="id"  type="bigint"  autoIncrement="true">
                <constraints  primaryKey="true"  nullable="false"/>
            </column>
            <column  name="first_name"  type="varchar(128)">
                <constraints nullable="false"/>
            </column>
            <column  name="last_name"  type="varchar(128)">
                <constraints nullable="false"/>
            </column>
            <column name="specialty_id" type="bigint">
                <constraints foreignKeyName="fk_developer_specialty" references="specialtyEntity(id)"/>
            </column>
            <column  name="status"  type="varchar(8)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
