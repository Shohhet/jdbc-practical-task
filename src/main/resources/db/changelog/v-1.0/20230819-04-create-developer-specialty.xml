<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <changeSet id="4" author="shohhet">
        <createTable tableName="developer_skill">
            <column name="developer_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_developer_id" references="developerEntity(id)"/>
            </column>
            <column name="skill_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_skill_id" references="skillEntity(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
